<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:id="@+id/linear01"
	android:soundEffectsEnabled="false">

	<LinearLayout
		android:id="@+id/navi_bar_header"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:gravity="right"
		android:background="@drawable/mobage_gradation_background">

		<Button android:id="@+id/logo_button"
			android:layout_height="48dip"
			android:layout_width="140dip"
			android:layout_weight="1"
			android:enabled="true"
			android:focusable="true"
			android:layout_marginLeft="0dip"
			android:layout_marginRight="0dip"
			android:layout_marginBottom="0dip"
			android:layout_marginTop="0dip"
			android:background="@drawable/mobage_logo_portrait_state"
			android:soundEffectsEnabled="false"/>
		
		<View android:layout_height="0dip"
			android:layout_width="1dip"
			android:layout_weight="100"
			/>
		<Button android:id="@+id/close_button"
			android:layout_height="48dip"
			android:layout_width="80dip"
			android:layout_weight="1"
			android:enabled="true"
			android:focusable="true"
			android:gravity="center_vertical"
			android:layout_marginLeft="0dip"
			android:layout_marginRight="0dip"
			android:layout_marginBottom="0dip"
			android:layout_marginTop="0dip"
			android:background="@drawable/mobage_close_state"
			android:soundEffectsEnabled="false"/>

	</LinearLayout>
	<View android:layout_height="1dip"
		android:layout_width="fill_parent"
		android:background="#FF0a305b"
		/>

	<WebView
		android:id="@+id/web_view"
		android:layout_width="fill_parent"
		android:layout_height="0dip"
		android:layout_weight="1.0" 
		android:soundEffectsEnabled="false"
		/>

	<View android:layout_height="1dip"
		android:layout_width="fill_parent"
		android:background="#FF0a305b"
		/>
	<LinearLayout
    android:id="@+id/navi_bar_footer"
		android:orientation="horizontal"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:soundEffectsEnabled="false"
		android:background="@drawable/mobage_gradation_background">

		<Button android:id="@+id/home_button"
			android:layout_height="48dip"
			android:layout_width="60dip"
			android:layout_weight="1"
			android:enabled="true"
			android:focusable="true"
			android:layout_marginLeft="0dip"
			android:layout_marginRight="0dip"
			android:layout_marginBottom="0dip"
			android:layout_marginTop="0dip"
			android:background="@drawable/mobage_home_state"
			android:soundEffectsEnabled="false"/>
		<View android:layout_height="0dip"
			android:layout_width="1dip"
			android:layout_weight="100"
			/>
		<Button android:id="@+id/back_button"
			android:layout_height="48dip"
			android:layout_width="60dip"
			android:layout_weight="1"
			android:enabled="true"
			android:focusable="true"
			android:layout_marginLeft="0dip"
			android:layout_marginRight="0dip"
			android:layout_marginBottom="0dip"
			android:layout_marginTop="0dip"
			android:background="@drawable/mobage_back_state"
			android:soundEffectsEnabled="false"/>
		<View android:layout_height="0dip"
			android:layout_width="1dip"
			android:layout_weight="100"
			/>
		<Button android:id="@+id/reload_button"
			android:layout_height="48dip"
			android:layout_width="60dip"
			android:layout_weight="1"
			android:enabled="true"
			android:focusable="true"
			android:layout_marginLeft="0dip"
			android:layout_marginRight="0dip"
			android:layout_marginBottom="0dip"
			android:layout_marginTop="0dip"
			android:background="@drawable/mobage_reload_state"
			android:soundEffectsEnabled="false"/>
		<View android:layout_height="0dip"
			android:layout_width="1dip"
			android:layout_weight="100"
			/>
		<Button android:id="@+id/mypage_button"
			android:layout_height="48dip"
			android:layout_width="60dip"
			android:layout_weight="1"
			android:enabled="true"
			android:focusable="true"
			android:layout_marginLeft="0dip"
			android:layout_marginRight="0dip"
			android:layout_marginBottom="0dip"
			android:layout_marginTop="0dip"
			android:background="@drawable/mobage_mypage_state"
			android:soundEffectsEnabled="false"/>
		<View android:layout_height="0dip"
			android:layout_width="1dip"
			android:layout_weight="100"
			/>
        <Button android:id="@+id/game_button"
			android:layout_height="48dip"
			android:layout_width="60dip"
			android:layout_weight="1"
			android:enabled="true"
			android:focusable="true"
			android:layout_marginLeft="0dip"
			android:layout_marginRight="0dip"
			android:layout_marginBottom="0dip"
			android:layout_marginTop="0dip"
			android:background="@drawable/mobage_game_state"
			android:soundEffectsEnabled="false"/>
	</LinearLayout>
</LinearLayout>

